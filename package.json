{
  "name": "nuxt-svg-icon-sprite",
  "version": "1.0.2",
  "description": "Generate SVG symbol sprites for icons and illustrations.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dulnan/nuxt-svg-icon-sprite.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground --trace-warnings",
    "dev:debug": "nuxi dev playground --trace-warnings --inspect",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev:serve": "node playground/.output/server/index.mjs",
    "docs:dev": "vitepress dev docs --port 5000",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs --port 5000",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "npx nuxi typecheck"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.0",
    "node-html-parser": "^7.0.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.0.1",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.0",
    "@vitest/coverage-c8": "^0.28.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "nuxt": "^3.15.4",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "vitepress": "^1.0.0-rc.40",
    "vitest": "^1.2.2"
  }
}
